// End f file challenge for Java 
// Adam Plesca 
// 22/4/23

import java.io.*;
import java.util.*;

public class EndOfFile {

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int input = 1;
        while(sc.hasNext()){ 
           System.out.println(input+" "+sc.nextLine()); 
           input++;
        }
        sc.close();
    }
}
